{
  "manifest_version": 2,
  "name": "vk blocker",
  "version": "1.0",
  
  "icons": {
    "128": "icon.ico"
  },
  "content_scripts": [
    {
      "matches": [ "*://vk.com/*" ],
      "js": [ "blocker.js" ]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
      "browser_action": {
        "default_title": "vk_blocker",
        "default_icon": "icon.ico",
        "default_popup": "popup.html"
    }
}
